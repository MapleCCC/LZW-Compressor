[run]
command_line = -m pytest tests
source = LZW

[report]
# Below exlucded_lines value comes from reference: https://coverage.readthedocs.io/en/coverage-4.3.3/excluding.html#advanced-exclusion
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
